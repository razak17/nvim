" set packpath=$XDG_DATA_HOME/rvim/site
" set runtimepath=$XDG_CONFIG_HOME/rvim
" set runtimepath^=$XDG_CONFIG_HOME/rvim/after
" set runtimepath^=$XDG_DATA_HOME/rvim/site
" set runtimepath^=/usr/local/share/nvim/runtime

syntax off
filetype off

let g:loaded_matchit= 1 "$VIMRUNTIME/plugin/matchit.vim
let g:loaded_tutor_mode_plugin= 1 "$VIMRUNTIME/tutor/tutor.tutor
let g:loaded_logipat           = 1 "$VIMRUNTIME/plugin/logiPat.vim
let g:loaded_matchparen        = 1 "$VIMRUNTIME/plugin/matchparen.vim
let g:loaded_netrw             = 1 "$VIMRUNTIME/autoload/netrw.vim
let g:loaded_netrwFileHandlers = 1 "$VIMRUNTIME/autoload/netrwFileHandlers.vim
let g:loaded_netrwPlugin       = 1 "$VIMRUNTIME/plugin/netrwPlugin.vim
let g:loaded_netrwSettings     = 1 "$VIMRUNTIME/autoload/netrwSettings.vim
let g:loaded_rrhelper          = 1 "$VIMRUNTIME/plugin/rrhelper.vim
let g:loaded_spellfile_plugin  = 1 "$VIMRUNTIME/plugin/spellfile.vim
let g:loaded_sql_completion    = 1 "$VIMRUNTIME/autoload/sqlcomplete.vim
let g:loaded_syntax_completion = 1 "$VIMRUNTIME/autoload/syntaxcomplete.vim
let g:loaded_tar               = 1 "$VIMRUNTIME/autoload/tar.vim
let g:loaded_tarPlugin         = 1 "$VIMRUNTIME/plugin/tarPlugin.vim
let g:loaded_vimball           = 1 "$VIMRUNTIME/autoload/vimball.vim
let g:loaded_vimballPlugin     = 1 "$VIMRUNTIME/plugin/vimballPlugin.vim
let g:loaded_zip               = 1 "$VIMRUNTIME/autoload/zip.vim
let g:loaded_zipPlugin         = 1 "$VIMRUNTIME/plugin/zipPlugin.vim
let g:vimsyn_embed             = 1 "$VIMRUNTIME/syntax/vim.vim
let g:loaded_python_provider = 0
let g:loaded_ruby_provider = 0
let g:loaded_perl_provider = 0
let g:floating_window_winblend = 0
let g:floating_window_scaling_factor = 0
let g:neovim_remote = 1
"let g:rg_derive_root = true

" set exrc
" set secure
" set t_Co=256
" set shortmess+=c
" set iskeyword+=-
" set ruler
" set noerrorbells
" set magic
" set shell=/bin/zsh
" set encoding=utf-8
" set fileencoding=utf-8
" set guicursor=n-v-c-sm:block,i-ci-ve:block,r-cr-o:block
" set mouse=a
" set mousefocus
" set noswapfile
" set undofile
" set nobackup
" set nowritebackup
" set clipboard+=unnamedplus
" set hidden
" set number relativenumber
" set hlsearch
" set noshowcmd
" set formatoptions-=cro
" set laststatus=0
" set showtabline=0
" set nowrap
" set colorcolumn=""
" set nocursorline
" set noshowmode

" highlight! link SignColumn Normal
" highlight StatusLine guifg=NONE guibg=NONE guisp=NONE gui=NONE blend=NONE
" highlight TabLineFill guifg=NONE guibg=NONE guisp=NONE gui=NONE blend=NONE
" highlight Todo guifg=#AF4B57 guibg=NONE guisp=NONE gui=bold blend=NONE
" highlight COlorColumn guibg=#4E525C
" highlight CursorLine guibg=#4E525C
" highlight LineNr guifg=#5B6268

" let mapleader=" "
" nnoremap n j
" nnoremap z u
" nnoremap W q
" nnoremap <TAB> :bnext<CR>
" nnoremap <S-TAB> :bprevious<CR>
" nnoremap <C-z>    :undo<CR>
" xnoremap K :move '<-2<CR>gv-gv
" xnoremap J :move '>+0<CR>gv-gv
" xnoremap N :move '>+0<CR>gv-gv
" nnoremap <Leader>x :q<CR>
" nnoremap <Leader>y "+y
" vnoremap <Leader>y' "+y
" nnoremap <Leader>aY gg"+yG
" nnoremap <Leader>aV gg"+VG
" nnoremap <Leader>aD gg"+VG
" nnoremap <Leader>chw :h <C-R>=expand("<cword>")<CR><CR>
