{
  "snippet": {
    "prefix": ["au"],
    "body": ["autocmd $TM_SELECTED_TEXT"],
    "description": "Example for selected text"
  },
  "it": {
    "prefix": "it",
    "body": ["it('should ${1:do something}', function()", "\t${2}", "end)"]
  },
  "describe": {
    "prefix": "describe",
    "body": ["describe('${1:Title}', function()", "\t${2}", "end)"]
  },
  "doc": {
    "prefix": "doc",
    "body": [
      "--------------------------------------------------------------------------------",
      "-- ${0:document}",
      "--------------------------------------------------------------------------------"
    ]
  },
  "inspect": {
    "prefix": "vi",
    "body": ["print(\"${1}: \" .. vim.inspect(${1}))"]
  },
  "function": {
    "prefix": "fn",
    "body": ["local function ${1:name}(${2:args})", "\t${3}", "end"]
  }
}
