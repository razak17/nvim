{
  "consoleLog": {
    "prefix": "clg",
    "body": ["console.log(${1:first})"],
    "description": "Displays a message in the console"
  },
  "exd": {
    "prefix": "exd",
    "body": "export default ${tm_filename_base}",
    "description": "creates a react function component without proptypes"
  },
  "lred": {
    "prefix": "lred",
    "body": "console.log('%c${1}', 'font-weight:bold;color:red;');"
  },
  "lgreen": {
    "prefix": "lgreen",
    "body": "console.log('%c${1}', 'font-weight:bold;color:green;');"
  },
  "cl": {
    "prefix": "cl",
    "body": "console.log('$1: ', ${1});"
  },
  "expf": {
    "prefix": "expf",
    "body": "export ${1:default }function($2) {\n  $3\n\n};",
    "description": "Export function"
  },
  "im": {
    "prefix": "im",
    "body": "import ${1:foo} from '${2:bar}'",
    "description": "import from"
  },
  "imm": {
    "prefix": "imm",
    "body": "import { ${1:foo} } from '${2:bar}'",
    "description": "import {} from"
  },
  "ex": {
    "prefix": "ex",
    "body": "export ${1:default} ${2:bar}",
    "description": "export default"
  },
  "doc": {
    "prefix": "doc",
    "body": "/*******************************************************************************\n  ${1: Header}\n ******************************************************************************/",
    "description": "Header"
  }
}
