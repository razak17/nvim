{
  "go.buildOnSave": "off"
  "gopls": {
    // https://github.com/golang/tools/blob/master/gopls/doc/settings.md
    "ui.codelenses": {
      "enabled": true,
      "generate": true
      "gc_details": false,
      "test": true,
      "tidy": true,
    },
    "ui.diagnostic.analyses": {
      "unusedparams": true,
      "loopclosure": true
    },
    "build.directoryFilters": [
      "-node_modules"
    ],
    "ui.completion.usePlaceholders": true,
    "ui.diagnostic.staticcheck": true,

  }
}
